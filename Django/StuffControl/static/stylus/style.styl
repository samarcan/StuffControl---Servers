header_color = #676767
gris_claro = #A3A3A3
gris_mas_claro = #E1E1E1
gris_oscuro = #676767



html
body
	margin 0
	padding 0
body
	font-size 16px
a
	text-decoration none
img 
	vertical-align top

.inline-block
	display inline-block
.left
	float left
.right
	float right
.boder_bottom
	border-bottom 1px solid gris_claro
.relative
	position relative
.vertical-middle
	vertical-align middle
.nomarginpadding
	margin 0
	padding 0
.fixed
	position fixed
	top 0
	z-index 2
	width 100%
.main
	margin 3em 0 0 0
	padding-bottom 3em
	width 100%
.lateral_main
	display none
.contentmain 
	background-color white
	display block
	margin 0 auto
	width 90%


